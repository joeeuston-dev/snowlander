# Unraid Environment Configuration for SNOWLANDER
# Copy this file to .env and fill in your values

# Required: Your Discord Bot Token
DISCORD_TOKEN=your_bot_token_here

# Optional: Your Discord Guild/Server ID (for guild-specific commands)
DISCORD_GUILD_ID=your_guild_id_here

# Music Share Path (adjust to your Unraid music share name)
# Common paths:
# /mnt/user/Music
# /mnt/user/media/Music  
# /mnt/user/shares/Music
UNRAID_MUSIC_SHARE=/mnt/user/Music

# Bot Configuration (optional)
BOT_PREFIX=!
DEFAULT_VOLUME=0.5

# Web Interface Port (optional, defaults to 8000)
WEB_PORT=8000

# Debug mode (optional, defaults to false)
DEBUG=false
